<% @movies["items"].each do |movie| %>
<div>
  <p>タイトル: <%= link_to "#{movie["snippet"]["title"]}", "https://www.youtube.com/watch?v=#{ movie["id"]["videoId"] }", target: 'blank' %></p>
  <p>説明: <%= movie["snippet"]["description"] %></p>
  <iframe width="560" height="315" src="https://www.youtube.com/watch?v=#{ movie["id"]["videoId"] }" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
</div>
<% end %>
