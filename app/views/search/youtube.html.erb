<%= render 'return' %>
<div class="row">
  <% @movies["items"].each do |movie| %>
  <div class="col-md-4 col-md-offset-1 youtube">
    <p>タイトル: <%= link_to "#{movie["snippet"]["title"]}", "https://www.youtube.com/watch?v=#{ movie["id"]["videoId"] }", target: 'blank' %></p>
    <p>説明: <%= movie["snippet"]["description"] %></p>
    <iframe class="video_box" src="https://www.youtube.com/embed/<%= movie["id"]["videoId"] %>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
  </div>
  <% end %>
</div>
